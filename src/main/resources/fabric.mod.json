{
	"schemaVersion": 1,
	"id": "kassette",
	"version": "${version}",
	"name": "Kassette",
	"description": "Fabric mod to show media player information",
	"authors": [
		"Spadium"
	],
	"contact": {
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "LGPL-3.0",
	"icon": "assets/kassette/icon.png",
	"environment": "client",
	"entrypoints": {
		"client": [
			{
				"value": "com.spadium.kassette.Kassette",
				"adapter": "kotlin"
			}
		],
		"modmenu": []
	},
	"custom": {
		"modmenu": {
			"links": {
				"modmenu.github_releases": "https://github.com/Spadium/kassette/releases"
			},
			"update_checker": true
		}
	},
	"mixins": [
		"kassette.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21.5",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	}
}